<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dstrong.helloworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hello_world_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity2"
            android:label="@string/title_activity_2"
            android:parentActivityName=".Main"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dstrong.helloworld.Main" />
        </activity>
        <activity
            android:name=".Activity3"
            android:label="@string/title_activity_3"
            android:parentActivityName=".Main"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dstrong.helloworld.Main" />
        </activity>
        <activity
            android:name=".Provide_Info"
            android:label="@string/title_Provide_info"
            android:parentActivityName=".Activity3"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dstrong.helloworld.Activity3" />
            >
        </activity>
        <activity
            android:name=".SwipeActivity"
            android:label="@string/title_activity_swipe"
            android:parentActivityName=".Main"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dstrong.helloworld.Main" />
        </activity>
    </application>

</manifest>
